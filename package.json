{
  "name": "js-library-setup-guide",
  "version": "0.0.1",
  "description": "A JavaScript Library Setup Guide and Script",
  "license": "MIT",
  "author": "te schultz",
  "keywords": [
    "javascript",
    "js-library",
    "setup-guide",
    "setup-script"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/artisin/js-library-setup-guide"
  },
  "bugs": {
    "url": "https://github.com/artisin/js-library-setup-guide/issues"
