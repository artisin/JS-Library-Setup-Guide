    "release:patch": "yarn run test && bumped release patch",
    "start": "node __scripts__/setup/cli-prompt.js"
  },
  "devDependencies": {
    "commitizen": "^2.9.6",
    "cz-customizable-te": "^6.0.0",
    "fs-extra": "^4.0.2",
    "inquirer": "^3.0.6",
    "json-format": "^1.0.1",
    "lodash": "^4.17.4"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable-te"
    },
    "cz-customizable": {
      "config": "./.cz-config.js"
    }
  }
